		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8684]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8685]
				subType=enum:0
				text=string:Es gibt drei Bedingungen, die ein Mann erfüllen muss, um Rekrut bei der Miliz zu werden.
				name=string:Infos_Mil_4_JOIN_CONDI_Info_04_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8686]
		blockName=string:Infos_Mil_4_JOIN_CONDI_Info_04_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8687]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8688]
				subType=enum:0
				text=string:Zunächst musst den Umgang mit einer Waffe beherrschen und einen kräftigen Arm haben. Vorher brauchst du nicht mal mit jemanden darüber sprechen.
				name=string:Infos_Mil_4_JOIN_CONDI_Info_04_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8689]
		blockName=string:Infos_Mil_4_JOIN_CONDI_Info_04_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8690]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8691]
				subType=enum:0
				text=string:Wenn du dich dann zur Miliz meldest, wirst du zur Wache eingeteilt. Das kann alles mögliche bedeuten.
				name=string:Infos_Mil_4_JOIN_CONDI_Info_04_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8692]
		blockName=string:Infos_Mil_4_JOIN_CONDI_Info_04_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8693]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8694]
				subType=enum:0
				text=string:Entweder ein Tor bewachen, Patrouillie gehen, auf der Pallisade stehen, es gibt einen ganzen Haufen Arbeit mit der du beschäftigt werden kannst.
				name=string:Infos_Mil_4_JOIN_CONDI_Info_04_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8695]
		blockName=string:Infos_Mil_4_JOIN_CONDI_Info_04_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8696]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8697]
				subType=enum:0
				text=string:Schlägt dein Herz danach immer noch für die Miliz, dann darfst du gegen einen von uns antreten. Wenn du danach noch stehst, bist du dabei.
				name=string:Infos_Mil_4_JOIN_CONDI_Info_04_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8698]
		blockName=string:Infos_Mil_4_JOIN_CONDI_Info_04_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8699]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8700]
				subType=enum:0
				text=string:Aber denke daran, wenn du zur Miliz gehörst, dann ist Schluss mit dem faulen Leben. Dann ist Disziplin gefordert! Jeden Tag Meldung machen!
				name=string:Infos_Mil_4_JOIN_CONDI_Info_04_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8701]
